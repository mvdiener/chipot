<div id="toolbar">
  <div id="date-range">
      <form id="date-pick">
          <label for="Start">Start date</label>
          <input id="start-date" type="date" name="Start">
          <label for="End">End date</label>
          <input id="end-date" type="date" name="End">
          <input type="submit" value="Find">
      </form>
  </div>
</div>

<div id="main">
  <div id='map-section'>
      <input id="pac-input" class="controls" type="text" placeholder="Search by address...">
      <button id="start-draw">Draw</button>
      <div id="map-canvas">
      </div>
  </div>
  
  <div id="info-section">
    <div id="chart" ng-app="chartApp" ng-controller="PotHoleChartController">
        <linechart data="data" options="options" mode=""></linechart>
    </div>

    <div id="marketing">
      <div class="average">Average number of pot holes reported per day: <span id="avg-report"></span></div>
        <div class="average">Average number of pot holes patched per day: <span id="avg-patch"></span></div>
    </div>
  </div>
</div>