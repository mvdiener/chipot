<div id="main">
  <h1>Fill My Hole</h1>
</div>

<div id="date-range">
    <form id="date-pick">
        <input id="start-date" type="date" name="Start">
        <input id="end-date" type="date" name="End">
        <input type="submit" value="Find">
    </form>
</div>

<div id="chart" ng-app="chartApp" ng-controller="PotHoleChartController">
    <linechart data="data" options="options" mode=""></linechart>
</div>

<div id='map-section'>
    <input id="pac-input" class="controls" type="text" placeholder="Find your hole">
    <div class="input-control checkbox">
        <label>
            <input type="checkbox" id="filled"/>
            <span class="check"></span>
            Unfilled only
        </label>
    </div>
    <div id="map-canvas">
    </div>
</div>



